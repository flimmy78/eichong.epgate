package com.ec.epcore.service;

import java.math.BigDecimal;

public class OrderStatInfo {

	private String chOrCode;
	
	private boolean exceptionData;
	
	private int thirdType;//优惠类型
	private int couPonAmt;
	private int pkCouPon;
	private int realCouPonAmt;
	
	
	private int remainAmt;
	private int totalChargeAmt;
	private int totalServiceAmt;
	private int totalAmt;
	
	public OrderStatInfo()
	{
		
		chOrCode="";
		
		exceptionData=false;
		
		thirdType=0;//优惠类型
		couPonAmt=0;
		pkCouPon=0;
		realCouPonAmt=0;
		
		
		remainAmt=0;
		totalChargeAmt=0;
		totalServiceAmt=0;
		totalAmt=0;
	}

	public String getChOrCode() {
		return chOrCode;
	}

	public void setChOrCode(String chOrCode) {
		this.chOrCode = chOrCode;
	}

	public boolean isExceptionData() {
		return exceptionData;
	}

	public void setExceptionData(boolean exceptionData) {
		this.exceptionData = exceptionData;
	}

	public int getThirdType() {
		return thirdType;
	}

	public void setThirdType(int thirdType) {
		this.thirdType = thirdType;
	}

	public int getCouPonAmt() {
		return couPonAmt;
	}

	public void setCouPonAmt(int couPonAmt) {
		this.couPonAmt = couPonAmt;
	}

	public int getPkCouPon() {
		return pkCouPon;
	}

	public void setPkCouPon(int pkCouPon) {
		this.pkCouPon = pkCouPon;
	}

	public int getRealCouPonAmt() {
		return realCouPonAmt;
	}

	public void setRealCouPonAmt(int realCouPonAmt) {
		this.realCouPonAmt = realCouPonAmt;
	}

	public int getRemainAmt() {
		return remainAmt;
	}

	public void setRemainAmt(int remainAmt) {
		this.remainAmt = remainAmt;
	}

	public int getTotalChargeAmt() {
		return totalChargeAmt;
	}

	public void setTotalChargeAmt(int totalChargeAmt) {
		this.totalChargeAmt = totalChargeAmt;
	}

	public int getTotalServiceAmt() {
		return totalServiceAmt;
	}

	public void setTotalServiceAmt(int totalServiceAmt) {
		this.totalServiceAmt = totalServiceAmt;
	}

	public int getTotalAmt() {
		return totalAmt;
	}

	public void setTotalAmt(int totalAmt) {
		this.totalAmt = totalAmt;
	}
	
	
}
