<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ormcore.tbl.domain.Partner">
	<resultMap id="get-Partner-result" type="TblPartner">
		<!-- 主键-->
		<result property="pkPartner" column="pk_partner" />
		<!-- 合格充电桩伙伴名字-->
		<result property="partnername" column="partnername" />
		<!-- 合格充电桩伙伴Key-->
		<result property="partnerkey" column="partnerkey" />
		<!-- 合格充电桩伙伴Token-->
		<result property="partnertoken" column="partnertoken" />
		<!-- 合格充电桩伙伴注册日期-->
		<result property="registerdate" column="registerdate" />
		<!-- 合格充电桩伙伴为我们分配的编号-->
		<result property="partnerclientid" column="partnerclientid" />
		<!-- 1:天;2:小时-->
		<result property="partnerupdatecycletype" column="partnerupdatecycletype" />
		<!-- 数值-->
		<result property="partnerupdatecyclevalue" column="partnerupdatecyclevalue" />
		<!-- 合格充电桩伙伴API服务IP-->
		<result property="partnerclientip" column="partnerclientip" />
		<!-- 合格充电桩伙伴API服务Port-->
		<result property="partnerclientport" column="partnerclientport" />
		<!-- 合格充电桩伙伴API服务Key-->
		<result property="partnerclientkey" column="partnerclientkey" />
		<!-- 先付费后付费(1:先付费,2:后付费)-->
		<result property="paymod" column="paymod" />
		<!-- -->
		<result property="createdate" column="createdate" />
		<!-- 修改时间-->
		<result property="updatedate" column="updatedate" />
		<!-- 第三方获取token地址-->
		<result property="tokenurl" column="tokenurl" />
		<!-- 第三方获取token密匙-->
		<result property="tokensecret" column="tokensecret" />
		<!-- 接口传递密匙：消息密匙|消息密匙初始向量|签名密匙-->
		<result property="secret" column="secret" />
		<!-- 推送启动充电结果url-->
		<result property="pushstarturl" column="pushstarturl" />
		<!-- 推送停止充电结果url-->
		<result property="pushstopurl" column="pushstopurl" />
		<!-- 推送充电订单url-->
		<result property="pushorderurl" column="pushorderurl" />
		<!-- 推送充电设备状态url-->
		<result property="pushequipstatusurl" column="pushequipstatusurl" />
		<!-- 推送订单对账结果url-->
		<result property="pushordercheckurl" column="pushordercheckurl" />
		<!-- 推送充电状态url-->
		<result property="pushchargestatusurl" column="pushchargestatusurl" />
		<!-- 关联tbl_company 主键-->
		<result property="cpyId" column="cpy_id" />
	</resultMap>
	<sql id="Partner_Base_Column_List">
		<!-- 主键-->
		pk_partner,
		<!-- 合格充电桩伙伴名字-->
		partnername,
		<!-- 合格充电桩伙伴Key-->
		partnerkey,
		<!-- 合格充电桩伙伴Token-->
		partnertoken,
		<!-- 合格充电桩伙伴注册日期-->
		registerdate,
		<!-- 合格充电桩伙伴为我们分配的编号-->
		partnerclientid,
		<!-- 1:天;2:小时-->
		partnerupdatecycletype,
		<!-- 数值-->
		partnerupdatecyclevalue,
		<!-- 合格充电桩伙伴API服务IP-->
		partnerclientip,
		<!-- 合格充电桩伙伴API服务Port-->
		partnerclientport,
		<!-- 合格充电桩伙伴API服务Key-->
		partnerclientkey,
		<!-- 先付费后付费(1:先付费,2:后付费)-->
		paymod,
		<!-- -->
		createdate,
		<!-- 修改时间-->
		updatedate,
		<!-- 第三方获取token地址-->
		tokenurl,
		<!-- 第三方获取token密匙-->
		tokensecret,
		<!-- 接口传递密匙：消息密匙|消息密匙初始向量|签名密匙-->
		secret,
		<!-- 推送启动充电结果url-->
		pushstarturl,
		<!-- 推送停止充电结果url-->
		pushstopurl,
		<!-- 推送充电订单url-->
		pushorderurl,
		<!-- 推送充电设备状态url-->
		pushequipstatusurl,
		<!-- 推送订单对账结果url-->
		pushordercheckurl,
		<!-- 推送充电状态url-->
		pushchargestatusurl,
		<!-- 关联tbl_company 主键-->
		cpy_id,
	</sql>

    <select id="Partner_custlist"  resultMap="get-Partner-result">
        SELECT
	        <include refid="Partner_Base_Column_List" />
        FROM
		tbl_partner
        WHERE valid='1'
		            <if test="pkPartner != null and pkPartner != ''">
                AND pk_partner = #{pkPartner}
            </if>
		            <if test="partnername != null and partnername != ''">
                AND partnername = #{partnername}
            </if>
		            <if test="partnerkey != null and partnerkey != ''">
                AND partnerkey = #{partnerkey}
            </if>
		            <if test="partnertoken != null and partnertoken != ''">
                AND partnertoken = #{partnertoken}
            </if>
		            <if test="registerdate != null and registerdate != ''">
                AND registerdate = #{registerdate}
            </if>
		            <if test="partnerclientid != null and partnerclientid != ''">
                AND partnerclientid = #{partnerclientid}
            </if>
		            <if test="partnerupdatecycletype != null and partnerupdatecycletype != ''">
                AND partnerupdatecycletype = #{partnerupdatecycletype}
            </if>
		            <if test="partnerupdatecyclevalue != null and partnerupdatecyclevalue != ''">
                AND partnerupdatecyclevalue = #{partnerupdatecyclevalue}
            </if>
		            <if test="partnerclientip != null and partnerclientip != ''">
                AND partnerclientip = #{partnerclientip}
            </if>
		            <if test="partnerclientport != null and partnerclientport != ''">
                AND partnerclientport = #{partnerclientport}
            </if>
		            <if test="partnerclientkey != null and partnerclientkey != ''">
                AND partnerclientkey = #{partnerclientkey}
            </if>
		            <if test="paymod != null and paymod != ''">
                AND paymod = #{paymod}
            </if>
		            <if test="createdate != null and createdate != ''">
                AND createdate = #{createdate}
            </if>
		            <if test="updatedate != null and updatedate != ''">
                AND updatedate = #{updatedate}
            </if>
		            <if test="tokenurl != null and tokenurl != ''">
                AND tokenurl = #{tokenurl}
            </if>
		            <if test="tokensecret != null and tokensecret != ''">
                AND tokensecret = #{tokensecret}
            </if>
		            <if test="secret != null and secret != ''">
                AND secret = #{secret}
            </if>
		            <if test="pushstarturl != null and pushstarturl != ''">
                AND pushstarturl = #{pushstarturl}
            </if>
		            <if test="pushstopurl != null and pushstopurl != ''">
                AND pushstopurl = #{pushstopurl}
            </if>
		            <if test="pushorderurl != null and pushorderurl != ''">
                AND pushorderurl = #{pushorderurl}
            </if>
		            <if test="pushequipstatusurl != null and pushequipstatusurl != ''">
                AND pushequipstatusurl = #{pushequipstatusurl}
            </if>
		            <if test="pushordercheckurl != null and pushordercheckurl != ''">
                AND pushordercheckurl = #{pushordercheckurl}
            </if>
		            <if test="pushchargestatusurl != null and pushchargestatusurl != ''">
                AND pushchargestatusurl = #{pushchargestatusurl}
            </if>
		            <if test="cpyId != null and cpyId != ''">
                AND cpy_id = #{cpyId}
            </if>
		    </select>
</mapper>
